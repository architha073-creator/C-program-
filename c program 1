#include <stdio.h>

int main() {
    int i, num, max, n, sum = 0;
    printf("Enter 10 numbers:\n");
    scanf("%d", &max);
    for(i = 1; i < 10; i++) {
        scanf("%d", &num);
        if(num > max)
            max = num;
    }
    printf("Maximum = %d\n", max);
    printf("Enter n: ");
    scanf("%d", &n);
    for(i = 1; i <= n; i++)
        sum += i;

    printf("Sum = %d\n", sum);

    return 0;
}
